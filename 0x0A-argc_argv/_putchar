#include <stdio.h>
                
#include "main.h"
                
                
/**
                
* _putchar - A function that writes
* the character c to stdout
                
* @c: A char to be  printed
                               
* Return: On success 1.
                
* On error, otherwise return -1
*errno is set appropriately.
                
*/
                
int _putchar(char c)
                
{
                
return (write(1, &c, 1));
                
}

